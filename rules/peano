N2 :: X + 1 = Y + 1 --> X = Y
A0 :: 0 + 1 --> 1
A1 :: X + 0 --> X
A2 :: X + ( Y + 1 ) --> ( X + Y ) + 1
M1 :: X * 0 --> 0
M2 :: X * ( Y + 1 ) --> ( X * Y ) + X
E1 :: X ^ 0 --> 1
E2 :: X ^ ( Y + 1 ) --> X ^ Y * X
01 :: X <= 0 	 --> X = 0
02 :: X <= Y + 1 --> ( X <= Y ) || ( X = Y + 1 )
INDa :: a ( 0 ) && a ( X ) => a ( X + 1 ) --> a ( Z )
S :: s ( X ) --> X + 1
N1 :: X + 1 = 0 --> false